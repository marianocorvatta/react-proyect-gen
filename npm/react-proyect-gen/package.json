{
    "name": "react-proyect-gen",
    "version": "0.1.2",
    "description": "Simple CLI to create react projects",
    "bin": "lib/index.js",
    "keywords": [
      "CLI",
      "react",
      "proyect"
    ],
    "scripts": {
      "typecheck": "tsc --noEmit",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "build": "tsc",
      "dev": "npm run build && node lib/index.js"
    },
    "author": "Mariano Corvatta <mariano.corvatta@gmail.com>",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/marianocorvatta/react-proyect-gen.git"
    },
    "bugs": {
      "url": "https://github.com/marianocorvatta/react-proyect-gen/issues",
      "email": "mariano.corvatta@gmail.com"
    },
    "homepage": "https://github.com/marianocorvatta/react-proyect-gen#readme",
    "devDependencies": {
      "@types/node": "^18.11.18",
      "@typescript-eslint/eslint-plugin": "^5.48.0",
      "@typescript-eslint/parser": "^5.48.0",
      "eslint": "^8.31.0",
      "typescript": "^4.9.4"
    },
    "optionalDependencies": {
      "react-proyect-gen-linux-x64": "0.1.1",
      "react-proyect-gen-linux-arm64": "0.1.1",
      "react-proyect-gen-darwin-x64": "0.1.1",
      "react-proyect-gen-darwin-arm64": "0.1.1",
      "react-proyect-gen-windows-x64": "0.1.1",
      "react-proyect-gen-windows-arm64": "0.1.1"
    },
    "eslintConfig": {
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "ignorePatterns": [
          "lib/*"
        ],
        "root": true
    }
}